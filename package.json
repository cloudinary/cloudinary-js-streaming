{
  "author": "Cloudinary <info@cloudinary.com>",
  "name": "@cloudinary/js-streaming",
  "description": "Cloudinary client-side package for live video streaming",
  "homepage": "http://cloudinary.com",
  "license": "MIT",
  "version": "1.0.0-beta.1",
  "main": "dist/js-streaming.js",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "test": "npm run server & node_modules/.bin/mocha test/bootstrap.js --recursive  \"test/**/*.test.js\" --timeout 20000",
    "pretest": "npm run build && npm run copybuild",
    "posttest": "del-cli test/utils/js-streaming.js",
    "copybuild": "copyfiles -f \"dist/js-streaming.js\" \"test/utils\" && echo 'build files copied.'\n",
    "server": "node_modules/.bin/http-server -p 9877 test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudinary/cloudinary-js-streaming.git"
  },
  "bugs": {
    "url": "https://github.com/cloudinary/cloudinary-js-streaming/issues"
  },
  "dependencies": {
    "es6-promise": "^4.2.8",
    "janus-gateway": "git://github.com/meetecho/janus-gateway.git#v0.8.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "copyfiles": "^2.1.1",
    "del-cli": "^3.0.0",
    "exports-loader": "^0.7.0",
    "http-server": "^0.12.0",
    "mocha": "^6.2.2",
    "puppeteer": "^2.0.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10"
  },
  "files": [
    "dist/cloudinary-live-stream.js",
    "dist/cloudinary-live-stream.js.map"
  ],
  "keywords": [
    "cloudinary",
    "streaming",
    "live",
    "video"
  ],
  "private": false
}
